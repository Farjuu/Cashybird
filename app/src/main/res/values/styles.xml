<resources xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:name="http://schemas.android.com/apk/res/android">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
<!--        <item name="android:windowDisablePreview">true</item>-->
    </style>
    <style name="AppTheme.Launcher">
        <item name="background">@drawable/gameoverback</item>
    </style>

    <style name="MainAppTheme" parent="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

</resources>
